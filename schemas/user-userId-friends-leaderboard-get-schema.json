{
    "$schema": "http://json-schema.org/draft-03/schema",
    "type": "object",
    "properties": {
    	"hideMeFromLeaderboard": {
    		"required": false,
    		"type": "boolean"
    	},
        "friends": {
            "items": {
                "properties": {
                    "average": {
                        "properties": {
                            "steps": {
                                "type": "integer"
                            }
                        },
                        "type": "object"
                    },
                    "rank": {
                        "properties": {
                            "steps": {
                                "type": "integer"
                            }
                        },
                        "type": "object"
                    },
                    "summary": {
                        "properties": {
                            "steps": {
                                "type": "integer"
                            }
                        },
                        "type": "object"
                    },
                    "lastUpdateTime": {
                        "type": "string"
                    },
                    "user": {
                        "properties": {
                            "aboutMe": {
                                "type": "string"
                            },
                            "avatar": {
                                "type": "string"
                            },
                            "city": {
                                "type": "string"
                            },
                            "country": {
                                "type": "string"
                            },
                            "dateOfBirth": {
                                "type": "string"
                            },
                            "displayName": {
                                "type": "string"
                            },
                            "encodedId": {
                                "type": "string"
                            },
                            "fullName": {
                                "type": "string"
                            },
                            "gender": {
                                "enum": [
                                    "MALE",
                                    "FEMALE"
                                ]
                            },
                            "height": {
                                "type": "number"
                            },
                            "nickname": {
                                "type": "string"
                            },
                            "offsetFromUTCMillis": {
                                "type": "integer"
                            },
                            "state": {
                                "type": "string",
                                "minLength": 2,
                                "maxLength": 2
                            },
                            "timezone": {
                                "type": "string"
                            },
                            "weight": {
                                "type": "number"
                            }
                        },
                        "type": "object"
                    }
                },
                "type": "object"
            },
        	"type": "array"
        }
    }
}